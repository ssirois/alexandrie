{
  "name": "alexandrie",
  "version": "1.0.0",
  "description": "A workflow to handle the administration and archiving of photos took at an event",
  "main": "dist/main.js",
  "scripts": {
    "app": "electron .",
    "build:back": "parcel build --target electron main.ts",
    "build:front": "parcel build --target electron --public-url ./ index.html",
    "build": "npm run build:front && npm run build:back",
    "lint": "tslint src/index.tsx",
    "serve": "parcel index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "watch:back": "parcel watch --target electron main.ts",
    "watch:front": "parcel watch --no-hmr --public-url ./ index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssirois/alexandrie.git"
  },
  "author": "Samuel D. Sirois",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ssirois/alexandrie/issues"
  },
  "homepage": "https://github.com/ssirois/alexandrie#readme",
  "devDependencies": {
    "@jest-runner/electron": "^1.1.1",
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "electron": "^4.1.4",
    "jest": "^24.7.1",
    "mock-fs": "^4.8.0",
    "parcel-bundler": "^1.12.3",
    "spectron": "^5.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
